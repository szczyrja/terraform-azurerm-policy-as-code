jsonencode(
                {
                  effect = {
                      allowedValues = [
                          "AuditIfNotExists",
                          "Disabled",
                        ]
                      defaultValue  = "AuditIfNotExists"
                      metadata      = {
                          description = "Enable or disable the execution of the policy"
                          displayName = "Effect"
                        }
                      type          = "String"
                    }
                }
            )